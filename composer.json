{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "name": "wpb/string-blade-compiler",
            "url": "https://github.com/TerrePorter/StringBladeCompiler.git",
            "type": "git"
        }
    ],
    "require": {
        "laravel/framework": "4.2.*",
        "vatsim/sso": "1.*",
        "nesbot/Carbon": "*",
        "vatsim/xml": "dev-master",
        "laravelbook/ardent": "2.*",
        "fitztrev/laravel-html-minify": "1.*",
        "flynsarmy/db-blade-compiler": "1.*",
        "wpb/string-blade-compiler": "1.*@dev",
        "guzzlehttp/guzzle": "~4.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/enums",
            "app/interfaces",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan migrate"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan migrate"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
